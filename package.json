{
  "name": "basic-electron-react-boilerplate",
  "version": "0.7.0",
  "description": "Minimal and modern react+electron+webpack boilerplate",
  "author": "Phillip Barbiero",
  "homepage": "https://github.com/pbarbiero/basic-electron-react-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbarbiero/basic-electron-react-boilerplate.git"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "build": "webpack --config webpack.build.config.js",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "lint": "eslint src/* --fix --quiet",
    "package": "webpack --config webpack.build.config.js",
    "postpackage": "electron-packager ./ --out=./builds",
    "prod": "webpack --config webpack.build.config.js && electron --noDevServer ."
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.1",
    "electron": "^1.7.8",
    "electron-packager": "^9.1.0",
    "eslint": "^4.6.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-require": "^0.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.28.0",
    "humps": "^2.0.1",
    "node-sass": "^4.7.2",
    "normalize-scss": "^7.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-redux-i18n": "^1.9.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.4.5"
  }
}
