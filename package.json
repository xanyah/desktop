{
  "name": "Xanyah",
  "version": "0.7.0",
  "description": "Checkout software",
  "author": {
    "name": "Xanyah Team",
    "email": "sofiane@xanyah.io"
  },
  "contributors": [
    {
      "name": "Cl√©ment Montois",
      "email": "clement@xanyah.io"
